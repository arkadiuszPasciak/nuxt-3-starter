{
  "version": "1.0.0",
  "description": "Starter template with a lot of useful features",
  "main": "src/app.vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/arkadiuszPasciak/nuxt-3-starter"
  },
  "author": "Arkadiusz Pa≈õciak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arkadiuszPasciak/nuxt-3-starter/issues"
  },
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky prepare",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "lint:js": "eslint --ext \".ts,.js,.vue\" .",
    "lintfix:js": "eslint --fix --ext \".ts,.js,.vue\" .",
    "lint:styles": "stylelint **/*.{scss,vue}",
    "lintfix:styles": "stylelint **/*.{scss,vue} --fix",
    "lint:prettier": "prettier --check .",
    "lintfix:prettier": "prettier --write --list-different .",
    "clean": "rm -rf src/.nuxt dist .output",
    "generate:webfonts": "svgtofont --sources ./src/assets/webfonts/svg --output ./src/assets/webfonts/build",
    "packages-check-updates": "npx npm-check-updates",
    "packages-update": "npx npm-check-updates -u",
    "test": "vitest",
    "test:run": "vitest run",
    "check-types": "npx nuxi typecheck"
  },
  "devDependencies": {
    "@intlify/nuxt3": "^0.2.4",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@nuxt/test-utils-edge": "^3.0.1-rc.0-27817270.4f2bfbe",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@pinia/nuxt": "^0.4.4",
    "@pinia/testing": "^0.0.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0",
    "pinia": "^2.0.24",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  }
}
